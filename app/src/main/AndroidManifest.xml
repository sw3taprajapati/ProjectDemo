<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sweta.NCCS">

    <application
        android:name="com.example.sweta.MyApplication.application"
        android:allowBackup="true"
        android:icon="@mipmap/nccs_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/nccs_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.sweta.ViewPager.AboutUs"
            android:noHistory="true"
            android:theme="@style/AppTheme.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity" />
        <activity android:name=".ForgotPasswordActivity" />
        <activity android:name=".MyActivities.MainActivity" />
        <activity android:name=".SigninActivity" />
    </application>

</manifest>